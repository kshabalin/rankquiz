<div class="table-responsive">
  <table class="table table-striped table-sm">
    <thead>
    <tr>
      <th>id</th>
      <th>City</th>
      <th>Title</th>
      <th>Market</th>
    </tr>
    </thead>
    <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%=listing.id %></td>
        <td><%=listing.city %></td>
        <td><%=listing.title %></td>
        <td><%=listing.list_markets %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
